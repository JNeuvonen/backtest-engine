{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "backtest-engine",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "http": {
        "all": true
      },
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "writeFile": true,
        "readFile": true,
        "copyFile": true,
        "createDir": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true
      },
      "os": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.jneuvonen.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": ["binaries/**/*", ".env"]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "backtest-engine",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}
